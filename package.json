{
  "name": "near-self-verify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:contract": "npm run lint:contract && cd contracts/verification-db && cargo near build non-reproducible-wasm",
    "deploy:testnet": "near contract deploy $NEAR_CONTRACT_ID use-file contracts/verification-db/target/near/verification_db.wasm without-init-call network-config testnet sign-with-keychain send",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:contract": "cd contracts/verification-db && cargo clippy --all-targets -- -D warnings",
    "lint:contract:fix": "cd contracts/verification-db && cargo clippy --fix --allow-dirty --allow-staged",
    "lint:fix": "eslint . --fix",
    "start": "next start"
  },
